<div id="class-select-container" class="class-select-scrollable">
    <% programClasses.forEach((classItem, index) => { %>
        <div class="form-check">
            <input class="form-check-input" type="radio" name="class-select" id="class<%= index %>" value="<%= classItem.id %>">
            <label class="form-check-label" for="class<%= index %>">
                <%= classItem.name %> - <%= classItem.professor %>
            </label>
        </div>
    <% }) %>
</div>